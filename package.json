{
  "name": "landingi-ui-kit-documentation",
  "private": true,
  "sideEffects": false,
  "description": "Landingi UI-Kit",
  "main": "src/package",
  "scripts": {
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "lint:all": "node_modules/.bin/eslint --fix -- ./src ./tests",
    "lint:ui": "node_modules/.bin/eslint --fix -- ./src",
    "lint:tests": "node_modules/.bin/eslint --fix  -- ./tests",
    "jest-runner": "node_modules/.bin/jest --maxWorkers=100%",
    "jest-runner:local": "node_modules/.bin/jest --colors --maxWorkers=50% --changedFilesWithAncestor",
    "test:unit": "npm-run-all lint:tests jest-runner",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.js",
    "format:tests": "prettier --write 'tests/**/*.{js}'",
    "format:shared": "prettier --write 'src/shared/**/*.{js}'",
    "test:coverage": "npm run jest-runner -- --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@sentry/browser": "6.10.0",
    "@sentry/react": "6.10.0",
    "@sentry/tracing": "6.10.0",
    "@u-wave/react-vimeo": "0.9.5",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "date-fns": "2.17.0",
    "detect-browser": "5.2.0",
    "docz": "2.3.1",
    "dompurify": "2.2.6",
    "draft-js": "0.11.7",
    "draft-js-export-html": "1.4.1",
    "formik": "2.2.9",
    "gatsby-plugin-sass": "4.10.0",
    "moment-timezone": "0.5.33",
    "node-sass": "6.0.1",
    "prop-types": "15.7.2",
    "qs": "6.10.1",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-date-range": "1.3.0",
    "react-dom": "17.0.2",
    "react-draft-wysiwyg": "1.14.7",
    "react-editor-js": "1.10.0",
    "react-image-annotation": "0.9.10",
    "react-ink": "6.4.0",
    "react-input-range": "1.3.0",
    "react-intl": "5.12.3",
    "react-pose": "4.0.10",
    "react-redux": "^7.2.4",
    "react-router-dom": "5.2.0",
    "react-select": "4.1.0",
    "react-table": "7.0.0-rc.15",
    "react-text-mask": "5.4.3",
    "react-tooltip": "4.2.21",
    "react-uuid": "1.0.2",
    "recharts": "2.0.10",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "redux-saga-routines": "^3.2.3",
    "retry-axios": "^2.4.0",
    "simple-flexbox": "2.3.3",
    "tiny-emitter": "2.1.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.12.17",
    "@babel/eslint-parser": "7.13.4",
    "@babel/eslint-plugin": "7.13.0",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-regenerator": "7.12.13",
    "@babel/plugin-transform-runtime": "7.12.17",
    "@babel/preset-env": "7.12.17",
    "@babel/preset-react": "7.12.13",
    "@babel/runtime": "7.12.18",
    "@testing-library/react-hooks": "5.0.3",
    "@wojtekmaj/enzyme-adapter-react-17": "0.4.1",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "babel-plugin-transform-imports": "2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "7.0.3",
    "css-loader": "5.0.2",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-webpack-plugin": "2.5.2",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.4",
    "jest-mock-axios": "4.3.0",
    "jest-prop-type-error": "1.1.0",
    "lint-staged": "10.5.4",
    "make-runnable": "1.3.8",
    "mini-css-extract-plugin": "1.3.8",
    "moxios": "0.4.0",
    "node-sass": "6.0.1",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "path": "0.12.7",
    "prettier": "2.3.2",
    "prettier-eslint-cli": "5.0.0",
    "react-styleguidist": "11.1.5",
    "redux-mock-store": "1.5.4",
    "redux-saga-test-plan": "4.0.1",
    "sass-loader": "10.1.1",
    "standard": "^16.0.3",
    "thread-loader": "3.0.1",
    "webpack": "5.24.2",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0",
    "webpack-merge": "5.7.3"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/tests/setup/setupTests.js",
      "<rootDir>/tests/setup/overrides.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/setup/",
      "<rootDir>/cypress",
      "jest-prop-type-error"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/setup/config/jest/fileMock.js",
      "\\.(scss)$": "identity-obj-proxy"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/client/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "jest-jasmine2",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "lint:all"
    ]
  },
  "engines": {
    "node": "node 14.17.3-alpine3.14",
    "npm": "6.14.13"
  }
}
