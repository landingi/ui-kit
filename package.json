{
    "name": "@landingi/landingi-ui-kit",
    "version": "1.0.0",
    "sideEffects": false,
    "description": "Landingi UI-Kit",
    "main": "dist",
    "source": "src/index.js",
    "scripts": {
        "docz:dev": "docz dev",
        "docz:build": "docz build",
        "docz:serve": "docz build && docz serve",
        "lint:all": "eslint --fix -- ./src ./tests",
        "lint:ui": "eslint --fix -- ./src ",
        "lint:tests": "eslint --fix -- ./tests",
        "jest-runner": "jest --colors --maxWorkers=100%",
        "jest-runner:local": "jest --colors --maxWorkers=50% --changedFilesWithAncestor",
        "test:unit": "jest-runner",
        "build": "cross-env webpack",
        "bundle": "cross-env webpack --config ./webpack.bundle.js",
        "format:tests": "prettier --write {,tests/**/}*.js",
        "format:src": "prettier --write {,src/**/}*.js",
        "test:coverage": "pnpm jest-runner -- --coverage",
        "pretty-quick": "pretty-quick --staged",
        "prepare": "husky install",
        "build:dev": "node ./config/packages/package.builder.js development && pnpm install && pnpm run build",
        "ci": "node ./config/packages/package.builder.js production && pnpm install && pnpm build"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-svg-core": "1.2.36",
        "@fortawesome/free-solid-svg-icons": "5.15.4",
        "@fortawesome/react-fontawesome": "0.1.15",
        "@u-wave/react-vimeo": "0.9.5",
        "classnames": "2.3.1",
        "date-fns": "2.24.0",
        "dompurify": "2.3.2",
        "formik": "2.2.9",
        "prop-types": "15.7.2",
        "qs": "6.10.1",
        "query-string": "7.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-router-dom": "5.3.0",
        "react-date-range": "1.4.0",
        "react-ink": "6.5.1",
        "react-intl": "5.20.10",
        "react-pose": "4.0.10",
        "react-select": "4.3.1",
        "react-table": "7.7.0",
        "react-text-mask": "5.4.3",
        "react-tooltip": "4.2.21",
        "react-uuid": "1.0.2",
        "recharts": "2.1.2",
        "simple-flexbox": "2.3.3",
        "tiny-emitter": "2.1.0",
        "yup": "0.32.9",
        "@babel/core": "7.15.5",
        "@babel/eslint-parser": "7.15.4",
        "@babel/eslint-plugin": "7.14.5",
        "@babel/plugin-proposal-class-properties": "7.14.5",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "@babel/plugin-transform-regenerator": "7.14.5",
        "@babel/plugin-transform-runtime": "7.15.0",
        "@babel/preset-env": "7.15.6",
        "@babel/preset-react": "7.14.5",
        "@babel/runtime": "7.15.4",
        "@testing-library/react-hooks": "7.0.2",
        "@wojtekmaj/enzyme-adapter-react-17": "0.6.3",
        "babel-jest": "27.2.0",
        "babel-loader": "8.2.2",
        "babel-plugin-module-resolver": "4.1.0",
        "babel-plugin-transform-dynamic-import": "2.1.0",
        "babel-plugin-transform-imports": "2.0.0",
        "clean-webpack-plugin": "4.0.0",
        "cross-env": "7.0.3",
        "css-loader": "6.2.0",
        "css-minimizer-webpack-plugin": "3.0.2",
        "docz": "2.3.1",
        "enzyme": "3.11.0",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-config-standard": "16.0.3",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-promise": "5.1.0",
        "eslint-plugin-react": "7.25.2",
        "eslint-plugin-react-hooks": "4.2.0",
        "eslint-webpack-plugin": "3.0.1",
        "gatsby-plugin-sass": "4.13.0",
        "husky": "7.0.2",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.2.0",
        "jest-prop-type-error": "1.1.0",
        "make-runnable": "1.3.10",
        "mini-css-extract-plugin": "2.3.0",
        "npm-run-all": "4.1.5",
        "optimize-css-assets-webpack-plugin": "6.0.1",
        "path": "0.12.7",
        "prettier": "2.4.1",
        "prettier-eslint-cli": "5.0.1",
        "pretty-quick": "^3.1.1",
        "sass": "1.41.1",
        "sass-loader": "12.1.0",
        "standard": "16.0.3",
        "terser-webpack-plugin": "^5.2.4",
        "thread-loader": "3.0.4",
        "webpack": "5.53.0",
        "webpack-bundle-analyzer": "4.4.2",
        "webpack-cli": "4.8.0",
        "webpack-merge": "5.8.0"
    },
    "jest": {
        "automock": false,
        "errorOnDeprecated": true,
        "maxConcurrency": 100,
        "setupFiles": [
            "<rootDir>/tests/setup/setupTests.js",
            "<rootDir>/tests/setup/overrides.js"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/tests/setup/",
            "jest-prop-type-error"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/setup/config/jest/fileMock.js",
            "\\.(scss)$": "identity-obj-proxy"
        },
        "collectCoverage": false,
        "collectCoverageFrom": [
            "src/**/*.{js}",
            "!src/client/**/*.{js}",
            "!**/node_modules/**",
            "!**/vendor/**"
        ],
        "testEnvironment": "jsdom",
        "testRunner": "jest-jasmine2",
        "coverageReporters": [
            "json-summary",
            "text",
            "lcov"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=16.9.1",
        "npm": ">=7.0.0"
    }
}
