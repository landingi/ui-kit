{
  "name": "landingi-ui-kit",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "main": ".src/index.js",
  "description": "Landingi UI-Kit",
  "scripts": {
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "lint:all": "eslint --fix -- ./src ./tests",
    "lint:ui": "eslint --fix -- ./src ",
    "lint:tests": "eslint --fix -- ./tests",
    "jest-runner": "jest --colors --maxWorkers=100%",
    "jest-runner:local": "jest --colors --maxWorkers=50% --changedFilesWithAncestor",
    "test:unit": "npm-run-all lint:tests jest-runner",
    "build": "cross-env webpack",
    "bundle": "cross-env webpack --config ./webpack.bundle.js",
    "format:tests": "prettier --write {,tests/**/}*.js",
    "format:shared": "prettier --write {,src/**/}*.js",
    "test:coverage": "npm run jest-runner -- --coverage",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install",
    "build:dev": "node ./config/packages/package.builder.js development && yarn install && yarn run build",
    "ci": "node ./config/packages/package.builder.js production && yarn install --frozen-lockfile && yarn run test:unit && yarn run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@u-wave/react-vimeo": "0.9.5",
    "classnames": "2.3.1",
    "date-fns": "2.23.0",
    "dompurify": "2.3.0",
    "formik": "2.2.9",
    "prop-types": "15.7.2",
    "qs": "6.10.1",
    "query-string": "7.0.1",
    "react-date-range": "1.3.0",
    "react-ink": "6.5.1",
    "react-input-range": "1.3.0",
    "react-intl": "5.20.6",
    "react-pose": "4.0.10",
    "react-select": "4.1.0",
    "react-table": "7.0.0-rc.15",
    "react-text-mask": "5.4.3",
    "react-tooltip": "4.2.21",
    "react-uuid": "1.0.2",
    "recharts": "2.0.10",
    "simple-flexbox": "2.3.3",
    "tiny-emitter": "2.1.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/eslint-parser": "7.14.7",
    "@babel/eslint-plugin": "7.14.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-regenerator": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-react": "7.14.5",
    "@babel/runtime": "7.14.8",
    "@testing-library/react-hooks": "7.0.1",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.3",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "babel-plugin-transform-imports": "2.0.0",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.2.0",
    "css-minimizer-webpack-plugin": "3.0.2",
    "docz": "2.3.1",
    "enzyme": "3.11.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-webpack-plugin": "3.0.1",
    "gatsby-plugin-sass": "4.10.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "jest-prop-type-error": "1.1.0",
    "make-runnable": "1.3.10",
    "mini-css-extract-plugin": "2.1.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "path": "0.12.7",
    "prettier": "2.3.2",
    "prettier-eslint-cli": "5.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "sass": "1.36.0",
    "sass-loader": "11.1.0",
    "standard": "16.0.3",
    "terser-webpack-plugin": "^5.1.4",
    "thread-loader": "3.0.4",
    "webpack": "5.46.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-merge": "5.8.0"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0"
  },
  "jest": {
    "automock": false,
    "errorOnDeprecated": true,
    "maxConcurrency": 100,
    "setupFiles": [
      "<rootDir>/tests/setup/setupTests.js",
      "<rootDir>/tests/setup/overrides.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/setup/",
      "jest-prop-type-error"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/setup/config/jest/fileMock.js",
      "\\.(scss)$": "identity-obj-proxy"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/client/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "jest-jasmine2",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "engines": {
    "node": "14.17.3",
    "npm": "6.14.13"
  }
}
