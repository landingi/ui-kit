{
  "name": "@landingi/landingi-ui-kit",
  "version": "1.2.17",
  "description": "Landingi UI-Kit",
  "main": "dist/app-uikit.js",
  "private": true,
  "license": "proprietary",
  "scripts": {
    "docz:dev": "yarn docz dev",
    "docz:build": " ./node_modules/.bin/docz build",
    "docz:serve": "docz build && docz serve",
    "lint": "eslint --fix -- ./src",
    "test:local": "jest --colors --changedFilesWithAncestor",
    "test": "jest --colors",
    "test:coverage": "yarn test --coverage --no-cache",
    "test:badges": "yarn test:coverage && jest-coverage-badges",
    "build": "rm -rf ./dist && yarn webpack",
    "bundle": "yarn webpack --config webpack.bundle.js",
    "format": "prettier --write {,src/**/}*.js",
    "pretty-quick": "pretty-quick --staged",
    "prepare": "husky install",
    "build:local": "node ./config/packages/package.builder.js production && yarn install && yarn build",
    "build:dev": "node ./config/packages/package.builder.js development && yarn install && yarn build",
    "cd:ui": "node ./config/packages/package.builder.js production && yarn build",
    "cd:docs": "node ./config/packages/package.builder.js documentation && yarn install && yarn docz:build",
    "release-it": "yarn version --patch && yarn build"
  },
