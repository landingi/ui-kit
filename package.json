{
  "name": "@landingi/landingi-ui-kit",
  "version": "2.0.0",
  "description": "Landingi UI-Kit",
  "private": true,
  "license": "proprietary",
  "scripts": {
    "docz:dev": "NODE_OPTIONS=\"--no-experimental-fetch --openssl-legacy-provider\" yarn docz dev",
    "docz:build": " ./node_modules/.bin/docz build",
    "docz:serve": "docz build && docz serve",
    "lint": "eslint --fix -- ./src",
    "test:local": "jest --colors --changedFilesWithAncestor",
    "test": "jest",
    "test:coverage": "yarn test --coverage --no-cache",
    "test:badges": "yarn test:coverage && jest-coverage-badges",
    "test:watch": "jest --watch",
    "format": "prettier --write {,src/**/}*.js",
    "pretty-quick": "pretty-quick --staged",
    "cd:ui": "node ./config/packages/package.builder.js production && yarn build",
    "cd:docs": "node ./config/packages/package.builder.js documentation && yarn install && yarn docz:build",
    "release-it": "yarn version --patch && yarn test:badges && yarn build",
    "rollup": "rm -rf ./dist && rollup -c"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
