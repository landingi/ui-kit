    "@typescript-eslint/parser": "^5.0.0",
    "babel-plugin-module-resolver": "4.1.0",
    "core-js": "3.22.5",
    "css-loader": "6.7.1",
    "docz": "2.4.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
